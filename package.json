{
  "name": "kaifstore",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest build && cd src/client && next build && cd ../../ && npm run start:prod",
    "start:dev": "dotenv -e .development.env -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node --optimize_for_size --max_old_space_size=2560 dist/main",
    "db:remigrate": "npm run db:migrate:undo && npm run db:migrate",
    "db:migrate": "dotenv -e .development.env node_modules/.bin/sequelize db:migrate",
    "db:create:dev": "dotenv -e .development.env node_modules/.bin/sequelize db:create",
    "db:seed:dev-undo-all": "dotenv -e .development.env node_modules/.bin/sequelize sequelize db:seed:undo:all",
    "db:drop:dev": "dotenv -e .development.env node_modules/.bin/sequelize db:drop",
    "db:migrate:undo": "dotenv -e .development.env node_modules/.bin/sequelize db:migrate:undo",
    "db:seed:dev": "dotenv -e .development.env node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "dotenv -e .development.env node_modules/.bin/sequelize db:seed:undo",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/styles": "^4.11.4",
    "@material-ui/system": "^4.12.1",
    "@mui/material": "^5.0.4",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.0.9",
    "@svgr/webpack": "^5.5.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.20",
    "babel-plugin-styled-components": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "faker": "^5.5.3",
    "i18next": "^21.2.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "lodash": "^4.17.21",
    "nestjs-sequelize-paginate": "^1.0.0",
    "next": "^11.1.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
